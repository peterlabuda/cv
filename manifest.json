{
	"sap.app": {
		"id": "sk.peterlabuda.Resume",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"resumeModel": {
				"uri": "model/resume.json",
				"type": "JSON"
			}
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sk.peterlabuda.Resume.view.Resume",
			"type": "XML",
			"async": true
		},
		
		"dependencies": {
			"libs": {
				"sap.m": {},
				"sap.uxap": {}
			}
		},
		"models": {
			"Resume": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "resumeModel"
		}
	},          
		"config": {
			"sample": {
				"stretch": true,
				"files": [
					"view/Resume.view.xml",
					"controller/Resume.controller.js",
					"model/resume.json",
					"Component.js",
					"manifest.json"
				]
			}
		}
	}
}